void main() {
  var jogador1 = "par";
  int escolhaJogador1 = 3;
  var jogador2 = "impar";
  int escolhaJogador2 = 2;

  int soma = escolhaJogador1 + escolhaJogador2;
  var resultado;

  if (soma % 2 == 0) {
    resultado = "par";
  } else {
    resultado = "impar";
  }

  if (jogador1 == "par" && resultado == "par") {
    print("Jogador 1 ganhou!");
  } else if (jogador1 == "impar" && resultado == "impar") {
    print("Jogador 1 ganhou!");
  } else {
    print("Jogador 2 ganhou!");
  }
}
