void main() {
  var escolhaJogador1 = "par";
  var numeroJogador1 = 3;
  var numeroJogador2 = 2;
  var soma = numeroJogador1 + numeroJogador2;

  if (soma % 2 == 0 && escolhaJogador1 == "par") {
    print("Jogador 1 ganhou!");
  } else if (soma % 2 != 0 && escolhaJogador1 == "impar") {
    print("Jogador 1 ganhou!");
  } else {
    print("Jogador 2 ganhou!");
  }
}
